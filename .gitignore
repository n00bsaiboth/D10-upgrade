# Ignore backups
/backups/*
/drush-backups/*

# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore scaffold files
# Use post-install and post-update commands to run @drupal-scaffold
/web/.csslintrc
/web/.editorconfig
/web/.eslintignore
/web/.eslintrc
/web/.eslintrc.json
/web/.gitattributes
/web/.gitignore
/web/.htaccess
/web/.ht.router.php
/web/autoload.php
/web/index.php
/web/robots.txt
/web/modules/.gitignore
/web/sites/default/default.settings.php
/web/sites/default/default.services.yml
/web/sites/development.services.yml
/web/sites/example.sites.php
/web/update.php
/web/web.config

# Ignore sensitive information
# Using the settings.php file to store the generic settings.
# The database settings are stored at the settings.local.php file.
/web/sites/*/settings.local.php

# Ignore Drupal's file directory
/web/sites/*/files/
/private/*
/tmp/*

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore links to gulp workflow files
node_modules
gulpfile.babel.js

# Ignore mac files
.DS_Store

# Linter
.phplint-cache

# SQL dump files
*.sql.gz
*.mysql
*.mysql.gz
/.editorconfig
/.gitattributes